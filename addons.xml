          <disclaimer lang="en_gb"></disclaimer>
          <language></language>
          <platform>all</platform>
          <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>
          <forum>http://forum.kodi.tv/showthread.php?tid=267160</forum>
